{
    "name": "dynzels/wp_custom",
    "description": "Installation custom de WordPress par Dynzels",
    "version": "2.0",
    "authors": [
        {
            "name": "Dynzels",
            "email": "dynzels@gmail.com",
            "homepage": "https://twitter.com/ychevrot",
            "role": "DÃ©veloppeur Web"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.4",
        "johnpbloch/wordpress": "5.*",
        "wpackagist-plugin/classic-editor": "*"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "dependencies": {
        "lodash": ">=4.17.13"
    }
}
